void main() {
  String nomeAluno = "Gabriel";
  double nota1 = 8;
  double nota2 = 7;
  
  double media = (nota1+nota2)/2;
  
  print("A media do aluno $nomeAluno é: $media ");
  
  if(media >= 6){
    print("O aluno está aprovado!!!");
  } else{
    print("O aluno está reprovado!!!");
  }
  
  
  
  
}
